struct VertexInput {
    position: float4 : POSITION,
}

struct PixelInput {
    position: float4 : SV_POSITION,
}

fn vertex_main(input: VertexInput) -> PixelInput {
    return PixelInput {
        position: input.position,
    };
}

fn fragment_main(input: PixelInput) -> float4 {
    return float4(1.0, 1.0, 1.0, 1.0);
}
